{
    "name": "takemo101/egg-skeleton",
    "type": "project",
    "description": "PHP Egg Skeleton",
    "keywords": [
        "php",
        "package",
        "web",
        "framework"
    ],
    "authors": [
        {
            "name": "takemo101",
            "email": "takemo101@gmail.com",
            "role": "Developer"
        }
    ],
    "homepage": "https://github.com/takemo101/egg",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\": "test/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "latte/latte": "^3.0",
        "symfony/validator": "^6.2",
        "microcmsio/microcms-php-sdk": "^1.0",
        "nesbot/carbon": "^2.66",
        "takemo101/egg": "^0.1.0"
    },
    "require-dev": {
        "php": "^8.1",
        "symfony/var-dumper": "^6.2",
        "friendsofphp/php-cs-fixer": "^3.14",
        "phpunit/phpunit": "^10.0",
        "phpstan/phpstan": "^1.9"
    },
    "repositories": [
        {
            "type": "path",
            "url": "module/*",
            "options": {
                "symlink": true
            }
        }
    ]
}
