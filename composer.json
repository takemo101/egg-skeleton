{
    "name": "egg/egg",
    "type": "project",
    "description": "PHP Egg Skeleton",
    "keywords": [
        "php",
        "package",
        "web",
        "framework"
    ],
    "authors": [
        {
            "name": "takemo101",
            "email": "takemo101@gmail.com",
            "role": "Developer"
        }
    ],
    "homepage": "https://github.com/takemo101/egg",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\": "test/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "latte/latte": "^3.0",
        "takemo101/egg": "^0.0.9",
        "symfony/validator": "^6.2"
    },
    "require-dev": {
        "php": "^8.1",
        "phpunit/phpunit": "^9.5",
        "phpstan/phpstan": "^1.4",
        "symfony/var-dumper": "^6.2",
        "friendsofphp/php-cs-fixer": "^3.13"
    }
}
